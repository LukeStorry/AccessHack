{
    "name": "simple_read",
    "description": "English-Simplification Chrome Addon for CSS's Accessibility Hackathon",
    "version": "0.5",

    "manifest_version": 2,
    "minimum_chrome_version": "16",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["jquery-3.2.1.min.js", "readabilityscore.js", "content.js"]
    }],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    }

}
